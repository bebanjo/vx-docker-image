{
  "builders": [
    {
      "type":        "docker",
      "image":       "ubuntu:12.04",
      "pull":        true,
      "export_path": "precise.tar"
    }
  ],
  "provisioners": [
    {
      "type":   "shell",
      "script": "docker/precise/base.sh"
    }
  ],
  "post-processors": [
    [
      {
        "type": "docker-import",
        "repository": "dmexe/vexor-precise",
        "tag": "latest"
      }
    ]
  ]
}
